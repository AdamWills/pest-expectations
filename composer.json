{
    "name": "spatie/pest-expectations",
    "description": "A collection of handy custom Pest expectations",
    "keywords": [
        "spatie",
        "pest-expectations"
    ],
    "homepage": "https://github.com/spatie/pest-expectations",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PestExpectations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PestExpectations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
